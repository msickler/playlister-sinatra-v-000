<h1> <%= artist.name %> </h1>

<h2>Songs:</h2>
<ul><% @artist.songs.each do |song| %>
  <strong><li><a href="/songs/<%= song.slug %>"> <%= song.name %> </a></li> </strong>
</ul>
 <% end %>

 <h3>Genres: <% @artist.genres.each do |genre| %></h3>
   <strong><a href="/genres/<%= genre.slug %>"> <%= genre.name %> </a></strong>
 <% end %>
